{
  "name": "@studiobear/shared-ui-themeui",
  "description": "Shared-UI Theme UI components for Studiobear Applications",
  "version": "0.1.0",
  "author": "Barry Low <barry@studiobear.co>",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index",
  "module": "./dist-esm/index",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@ionic/react": "^4.11.10",
    "@loadable/component": "^5.12.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.19.5",
    "gatsby-image": "^2.2.37",
    "ionicons": "^4.6.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-typography": "^0.16.19",
    "theme-ui": "^0.3.1",
    "typography": "^0.16.19",
    "typography-theme-st-annes": "^0.16.19"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-build": "tsc && tsc --build tsconfig.esm.json && yarn copy-styles",
    "copy-styles": "rm -rf ./dist/styles & rm -rf ./dist-esm/styles && cp -Rf ./src/styles ./dist/styles && cp -Rf ./src/styles ./dist-esm/styles",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.3.10",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-notes": "^5.3.10",
    "@storybook/addons": "^5.1.11",
    "@storybook/api": "^5.3.9",
    "@storybook/core": "^5.3.9",
    "@storybook/react": "^5.1.11",
    "@testing-library/jest-dom": "^5.0.2",
    "@types/loadable__component": "^5.10.0",
    "@types/react-helmet": "^5.0.15",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.4"
  }
}
