{
  "name": "gatsby-starter-ionic",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "A starter for Gatsby integrated with Ionic + Typescript and TDD-ready",
  "version": "0.0.1",
  "author": "Barry Low <barry@studiobear.co>",
  "dependencies": {
    "@studiobear/core": "file:packages/core",
    "@studiobear/gatsby-theme-blog": "file:packages/gatsby-theme-blog",
    "@studiobear/gatsby-theme-ionic": "file:packages/gatsby-theme-ionic"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^16.9.1",
    "@types/testing-library__cypress": "^5.0.1",
    "@types/testing-library__jest-dom": "git://github.com/vagonpidarasov/jest-dom.git",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "axe-core": "^3.4.1",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.26",
    "cypress": "^3.8.2",
    "cypress-axe": "^0.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gatsby-cypress": "^0.2.19",
    "husky": "^4.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-gatsby": "^1.0.26",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cd packages/core && yarn develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:all": "yarn test:core --passWithNoTests && yarn test:ionic --passWithNoTests",
    "test:core": "cd packages/core && yarn test",
    "test:blog": "cd packages/gatsby-theme-blog && yarn test",
    "test:ionic": "cd packages/gatsby-theme-ionic && yarn test",
    "test:watch": "yarn test --watch",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run format && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/studiobear/gatsby-starter-ionic.git"
  },
  "bugs": {
    "url": "https://gitlab.com/studiobear/gatsby-starter-ionic/issues"
  },
  "engines": {
    "node": "10.16.0"
  }
}
